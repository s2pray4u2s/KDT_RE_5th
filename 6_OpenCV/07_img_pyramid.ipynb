   "source": [
    "image = cv2.imread('images/Castorice.jpg')\n",
    "\n",
    "# 2배 확대\n",
    "image_up = cv2.pyrUp(image)\n",
    "cv2.imshow('PyrUp Image', image_up)\n",
    "\n",
    "# 2배 축소\n",
    "image_down = cv2.pyrDown(image)\n",
    "cv2.imshow('PyrDown Image', image_down)\n",
    "\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
  }
